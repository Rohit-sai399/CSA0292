#include <stdio.h>
#include<string.h>

int main()
{
 char s[]={"racecar"};
 int i,j;
 int len=strlen(s),ispalindrome=1;
 for(i=0,j=len-1;i<j;i++,j--)
 {
     if(s[i]!=s[j])
     {
         ispalindrome=0;
         break;
     }
 }
 if(ispalindrome)
 {
 printf("is palindrome");
 }
 else
 {
     printf("not");
 }
}
