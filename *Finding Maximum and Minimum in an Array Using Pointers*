#include <stdio.h>

int main()
{
    int arr[]={2,9,7,6,5};
    int*ptr=arr;
    int max=*ptr;
    int min=*ptr;
    int i;
    for(i=1;i<=4;i++)
    {
    if(*(ptr+i)>max)
    {
     max =*(ptr+i);
    }
    if(*(ptr+i)<min)
    {
        min=*(ptr-i);
    }
    }
    printf("max value=%d\n",max);
    printf("min value=%d",min);
    

   
   
}

