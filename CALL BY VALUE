#include<stdio.h>
void swap(int x, int y){
    int temp=x;
    x=y;
    y=temp;
    
}
int main()
{
    int a,b;
    printf("enter values of a and b\n");
    scanf("%d%d", &a,&b);
     printf("values of a and b before swapping a=%d and b=%d\n",a,b);
       swap(a,b);
    printf("values of a and b after swapping a=%d and b=%d\n",a,b);
    return 0;
}
