#include<stdio.h>
int main()
{
    int arr1[]={1,2,3,4,6};
    int arr2[]={1,2,3,4,5,6};
    int arr3[11];
    int i;
    for(i=0;i<5;i++)
    {
        arr3[i]=arr1[i];
    }
        
        for(i=0;i<6;i++)
        {
            arr3[5+i]=arr2[i];
        }
        for(i=0;i<=10;i++)
        {
            printf("%d ",arr3[i]);
        }
    }
