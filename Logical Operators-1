#include<stdio.h>

int main()
{
   int a;
   printf("enter the value of a\n");
   scanf("%d",&a);
   if(a>=10&&a<=20)
   {
       printf("a is in the range 10-20\n");
   }
   else
   {
       printf("out of range\n ");
   }
   int b;
   printf("enter the value of b\n");
   scanf("%d",&b);
   if(a>=0||b>=0)
   {
       printf("atleast one number is positive");
   }
   else
   {
       printf("both numbers are negative");
   }
}
